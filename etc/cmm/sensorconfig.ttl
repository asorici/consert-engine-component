      sensorconf:instanceType smartclassroom:LuminositySensor ;
    ] ;
  sensorconf:hasUpdateMode coordconf:time-based ;
  sensorconf:hasUpdateRate 10 ;
  rdfs:label "luminositySensingPolicy"^^xsd:string ;
