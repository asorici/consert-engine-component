      sensorconf:instanceID <http://pervasive.semanticweb.org/ont/2014/07/smartclassroom/bootstrap#Lum_EF210_Section2_Right> ;
      sensorconf:instanceType smartclassroom:LuminositySensor ;
    ] ;
