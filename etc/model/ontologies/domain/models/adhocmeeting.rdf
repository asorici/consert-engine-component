<?xml version="1.0"?>
<rdf:RDF
    xmlns:meeting="http://pervasive.semanticweb.org/ont/2004/06/meeting#"
    xmlns:contextconstraint="http://pervasive.semanticweb.org/ont/2014/05/consert/constraint#"
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:contextannotation="http://pervasive.semanticweb.org/ont/2014/05/consert/annotation#"
    xmlns:contextassertion="http://pervasive.semanticweb.org/ont/2014/05/consert/core#"
    xmlns:spin="http://spinrdf.org/spin#"
    xmlns:afn="http://jena.hpl.hp.com/ARQ/function#"
    xmlns="http://pervasive.semanticweb.org/ont/2013/09/adhocmeeting/models#"
    xmlns:cfn="http://pervasive.semanticweb.org/ont/2014/05/consert/functions#"
    xmlns:sp="http://spinrdf.org/sp#"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
    xmlns:device="http://pervasive.semanticweb.org/ont/2004/06/device#"
    xmlns:spl="http://spinrdf.org/spl#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
    xmlns:location="http://pervasive.semanticweb.org/ont/2004/06/location#"
  xml:base="http://pervasive.semanticweb.org/ont/2013/09/adhocmeeting/models">
  <owl:Ontology rdf:about="">
    <owl:imports rdf:resource="http://pervasive.semanticweb.org/ont/2014/05/consert/annotation"/>
    <owl:imports rdf:resource="http://pervasive.semanticweb.org/ont/2014/05/consert/constraint"/>
    <owl:imports rdf:resource="http://pervasive.semanticweb.org/ont/2004/06/action"/>
    <owl:imports rdf:resource="http://pervasive.semanticweb.org/ont/2014/05/consert/functions"/>
    <owl:imports rdf:resource="http://pervasive.semanticweb.org/ont/2004/06/meeting"/>
    <owl:imports rdf:resource="http://pervasive.semanticweb.org/ont/2014/05/consert/core"/>
    <owl:imports rdf:resource="http://pervasive.semanticweb.org/ont/2004/06/device"/>
    <owl:imports rdf:resource="http://pervasive.semanticweb.org/ont/2004/06/person"/>
    <owl:imports rdf:resource="http://pervasive.semanticweb.org/ont/2004/06/location"/>
  </owl:Ontology>
  <owl:Class rdf:ID="Microphone">
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Microphone</rdfs:label>
    <rdfs:subClassOf>
      <rdf:Description rdf:about="http://pervasive.semanticweb.org/ont/2004/06/device#Device">
        <owl:hasKey rdf:parseType="Collection">
          <rdf:Description rdf:about="http://pervasive.semanticweb.org/ont/2004/06/device#modelNumber"/>
          <rdf:Description rdf:about="http://pervasive.semanticweb.org/ont/2004/06/device#serialNumber"/>
        </owl:hasKey>
      </rdf:Description>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="BuildingFloor">
    <owl:disjointWith>
      <owl:Class rdf:ID="BuildingRoomSection"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:ID="BuildingCorp"/>
    </owl:disjointWith>
    <rdfs:subClassOf rdf:resource="http://pervasive.semanticweb.org/ont/2004/06/space#SpaceInAFixedStructure"/>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >BuildingFloor</rdfs:label>
    <owl:disjointWith>
      <owl:Class rdf:ID="BuildingRoom"/>
    </owl:disjointWith>
  </owl:Class>
  <owl:Class rdf:ID="Skeleton">
    <rdfs:subClassOf rdf:resource="http://pervasive.semanticweb.org/ont/2014/05/consert/core#ContextEntity"/>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Skeleton</rdfs:label>
    <owl:hasKey rdf:parseType="Collection">
      <owl:DatatypeProperty rdf:ID="hasTrackingID"/>
    </owl:hasKey>
  </owl:Class>
  <owl:Class rdf:ID="HostsAdHocMeeting">
    <rdfs:subClassOf rdf:resource="http://pervasive.semanticweb.org/ont/2014/05/consert/core#UnaryContextAssertion"/>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >HostsAdHocMeeting</rdfs:label>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:hasValue rdf:resource="http://pervasive.semanticweb.org/ont/2014/05/consert/core#Derived"/>
        <owl:onProperty>
          <owl:FunctionalProperty rdf:about="http://pervasive.semanticweb.org/ont/2014/05/consert/core#assertionType"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:FunctionalProperty rdf:about="http://pervasive.semanticweb.org/ont/2014/05/consert/core#assertionRole"/>
        </owl:onProperty>
        <owl:allValuesFrom>
          <owl:Class rdf:about="#BuildingRoomSection"/>
        </owl:allValuesFrom>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="#BuildingRoom">
    <owl:disjointWith>
      <owl:Class rdf:about="#BuildingRoomSection"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:about="#BuildingCorp"/>
    </owl:disjointWith>
    <rdfs:subClassOf rdf:resource="http://pervasive.semanticweb.org/ont/2004/06/space#SpaceInAFixedStructure"/>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >BuildingRoom</rdfs:label>
    <owl:disjointWith rdf:resource="#BuildingFloor"/>
  </owl:Class>
  <owl:Class rdf:about="#BuildingRoomSection">
    <spin:deriveassertion>
      <hostsAdHocMeetingConstruct/>
    </spin:deriveassertion>
    <owl:disjointWith rdf:resource="#BuildingFloor"/>
    <rdfs:subClassOf rdf:resource="http://pervasive.semanticweb.org/ont/2004/06/space#SpaceInAFixedStructure"/>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >BuildingRoomSection</rdfs:label>
    <owl:disjointWith rdf:resource="#BuildingRoom"/>
    <owl:disjointWith>
      <owl:Class rdf:about="#BuildingCorp"/>
    </owl:disjointWith>
  </owl:Class>
  <owl:Class rdf:ID="Building">
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Building</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://pervasive.semanticweb.org/ont/2004/06/space#FixedStructure"/>
  </owl:Class>
  <owl:Class rdf:ID="KinectCamera">
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >KinectCamera</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://pervasive.semanticweb.org/ont/2004/06/device#Device"/>
  </owl:Class>
  <owl:Class rdf:ID="SkeletonPosition">
    <rdfs:subClassOf rdf:resource="http://pervasive.semanticweb.org/ont/2014/05/consert/core#ContextEntity"/>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >SkeletonPosition</rdfs:label>
    <owl:oneOf rdf:parseType="Collection">
      <SkeletonPosition rdf:ID="crouching">
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
        >SkeletonPosition</rdfs:label>
      </SkeletonPosition>
      <SkeletonPosition rdf:ID="leaning">
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
        >SkeletonPosition</rdfs:label>
      </SkeletonPosition>
      <SkeletonPosition rdf:ID="sitting">
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
        >SkeletonPosition</rdfs:label>
      </SkeletonPosition>
      <SkeletonPosition rdf:ID="standing">
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
        >SkeletonPosition</rdfs:label>
      </SkeletonPosition>
      <SkeletonPosition rdf:ID="walking">
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
        >SkeletonPosition</rdfs:label>
      </SkeletonPosition>
    </owl:oneOf>
  </owl:Class>
  <owl:Class rdf:about="#BuildingCorp">
    <owl:disjointWith rdf:resource="#BuildingFloor"/>
    <owl:disjointWith rdf:resource="#BuildingRoomSection"/>
    <rdfs:subClassOf rdf:resource="http://pervasive.semanticweb.org/ont/2004/06/space#SpaceInAFixedStructure"/>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >BuildingCorp</rdfs:label>
    <owl:disjointWith rdf:resource="#BuildingRoom"/>
  </owl:Class>
  <owl:Class rdf:ID="SensesSkelInPosition">
    <rdfs:subClassOf rdf:resource="http://pervasive.semanticweb.org/ont/2014/05/consert/core#NaryContextAssertion"/>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >SensesSkelInPosition</rdfs:label>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:hasValue rdf:resource="http://pervasive.semanticweb.org/ont/2014/05/consert/core#Sensed"/>
        <owl:onProperty rdf:resource="http://pervasive.semanticweb.org/ont/2014/05/consert/core#assertionType"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <owl:hasKey rdf:parseType="Collection">
      <owl:ObjectProperty rdf:ID="kinectCameraRole"/>
      <owl:ObjectProperty rdf:ID="skeletonRole"/>
    </owl:hasKey>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="#kinectCameraRole"/>
        </owl:onProperty>
        <owl:allValuesFrom rdf:resource="#KinectCamera"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="#skeletonRole"/>
        </owl:onProperty>
        <owl:allValuesFrom rdf:resource="#Skeleton"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:ObjectProperty rdf:ID="skelPositionRole"/>
        </owl:onProperty>
        <owl:allValuesFrom rdf:resource="#SkeletonPosition"/>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:ObjectProperty rdf:about="#skelPositionRole">
    <rdfs:subPropertyOf rdf:resource="http://pervasive.semanticweb.org/ont/2014/05/consert/core#assertionRole"/>
    <rdfs:range rdf:resource="#SkeletonPosition"/>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >skelPositionRole</rdfs:label>
    <rdfs:domain rdf:resource="#SensesSkelInPosition"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="#kinectCameraRole">
    <rdfs:subPropertyOf rdf:resource="http://pervasive.semanticweb.org/ont/2014/05/consert/core#assertionRole"/>
    <rdfs:range rdf:resource="#KinectCamera"/>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >SkeletonPosition</rdfs:label>
    <rdfs:domain rdf:resource="#SensesSkelInPosition"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="personRoomSection">
    <rdfs:subPropertyOf>
      <rdf:Description rdf:about="http://pervasive.semanticweb.org/ont/2014/05/consert/core#sensedRelationAssertion">
        <spin:abstract rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean"
        >true</spin:abstract>
      </rdf:Description>
    </rdfs:subPropertyOf>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >personRoomSection</rdfs:label>
    <rdfs:domain>
      <rdf:Description rdf:about="http://pervasive.semanticweb.org/ont/2004/06/person#Person">
        <spin:contextconstraint>
          <uniquePersonLocationConstraint/>
        </spin:contextconstraint>
      </rdf:Description>
    </rdfs:domain>
    <rdfs:range rdf:resource="#BuildingRoomSection"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="roomSectionRole">
    <rdfs:subPropertyOf rdf:resource="http://pervasive.semanticweb.org/ont/2014/05/consert/core#assertionRole"/>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >roomSectionRole</rdfs:label>
    <rdfs:domain rdf:resource="#HostsAdHocMeeting"/>
    <rdfs:range rdf:resource="#BuildingRoomSection"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="deviceRoomSection">
    <rdfs:subPropertyOf>
      <rdf:Description rdf:about="http://pervasive.semanticweb.org/ont/2014/05/consert/core#profiledRelationAssertion">
        <spin:abstract rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean"
        >true</spin:abstract>
      </rdf:Description>
    </rdfs:subPropertyOf>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >deviceRoomSection</rdfs:label>
    <rdfs:domain rdf:resource="http://pervasive.semanticweb.org/ont/2004/06/device#Device"/>
    <rdfs:range rdf:resource="#BuildingRoomSection"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="#skeletonRole">
    <rdfs:subPropertyOf rdf:resource="http://pervasive.semanticweb.org/ont/2014/05/consert/core#assertionRole"/>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >skeletonRole</rdfs:label>
    <rdfs:range rdf:resource="#Skeleton"/>
    <rdfs:domain rdf:resource="#SensesSkelInPosition"/>
  </owl:ObjectProperty>
  <rdf:Property rdf:about="http://spinrdf.org/arg#graphUUID">
    <rdfs:subPropertyOf rdf:resource="http://spinrdf.org/sp#arg"/>
  </rdf:Property>
  <rdf:Property rdf:about="http://spinrdf.org/arg#intervalSet1">
    <rdfs:subPropertyOf rdf:resource="http://spinrdf.org/sp#arg"/>
  </rdf:Property>
  <owl:DatatypeProperty rdf:ID="hasNoiseLevel">
    <rdfs:subPropertyOf rdf:resource="http://pervasive.semanticweb.org/ont/2014/05/consert/core#sensedDataAssertion"/>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >hasNoiseLevel</rdfs:label>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#double"/>
    <rdfs:domain rdf:resource="#Microphone"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="#hasTrackingID">
    <rdfs:subPropertyOf rdf:resource="http://pervasive.semanticweb.org/ont/2014/05/consert/core#entityDataDescription"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >hasTrackingID</rdfs:label>
  </owl:DatatypeProperty>
  <rdf:Description rdf:about="http://pervasive.semanticweb.org/ont/2014/05/consert/core#dynamicRelationAssertion">
    <spin:abstract rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean"
    >true</spin:abstract>
  </rdf:Description>
  <rdf:Description rdf:about="http://pervasive.semanticweb.org/ont/2014/05/consert/core#entityRelationAssertion">
    <spin:abstract rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean"
    >true</spin:abstract>
  </rdf:Description>
  <hostsAdHocMeetingConstruct/>
  <rdf:Description rdf:about="http://pervasive.semanticweb.org/ont/2014/05/consert/core#derivedRelationAssertion">
    <spin:abstract rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean"
    >true</spin:abstract>
  </rdf:Description>
  <rdf:Description rdf:about="http://pervasive.semanticweb.org/ont/2014/05/consert/core#staticRelationAssertion">
    <spin:abstract rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean"
    >true</spin:abstract>
  </rdf:Description>
  <spin:UpdateTemplate rdf:ID="hostsAdHocMeeting">
    <spin:body>
      <sp:Modify>
        <sp:where rdf:parseType="Collection">
          <sp:NamedGraph>
            <sp:elements rdf:parseType="Collection">
              <rdf:Description>
                <sp:object rdf:resource="#KinectCamera"/>
                <sp:predicate rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#type"/>
                <sp:subject rdf:parseType="Resource">
                  <sp:varName rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                  >K</sp:varName>
                </sp:subject>
              </rdf:Description>
              <rdf:Description>
                <sp:object rdf:resource="#BuildingRoomSection"/>
                <sp:predicate rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#type"/>
                <sp:subject rdf:parseType="Resource">
                  <sp:varName rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                  >RL</sp:varName>
                </sp:subject>
              </rdf:Description>
            </sp:elements>
            <sp:graphNameNode rdf:resource="http://pervasive.semanticweb.org/ont/2013/09/adhocmeeting/models/entityStore"/>
          </sp:NamedGraph>
          <sp:NamedGraph>
            <sp:elements rdf:parseType="Collection">
              <rdf:Description>
                <sp:object rdf:parseType="Resource">
                  <sp:varName rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                  >RL</sp:varName>
                </sp:object>
                <sp:predicate rdf:resource="#deviceRoomSection"/>
                <sp:subject rdf:parseType="Resource">
                  <sp:varName rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                  >K</sp:varName>
                </sp:subject>
              </rdf:Description>
            </sp:elements>
            <sp:graphNameNode rdf:parseType="Resource">
              <sp:varName rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
              >gRoomSection</sp:varName>
            </sp:graphNameNode>
          </sp:NamedGraph>
          <sp:NamedGraph>
            <sp:elements rdf:parseType="Collection">
              <rdf:Description>
                <sp:object rdf:resource="http://pervasive.semanticweb.org/ont/2014/05/consert/core#Profiled"/>
                <sp:predicate rdf:resource="http://pervasive.semanticweb.org/ont/2014/05/consert/core#assertionType"/>
                <sp:subject rdf:parseType="Resource">
                  <sp:varName rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                  >gRoomSection</sp:varName>
                </sp:subject>
              </rdf:Description>
            </sp:elements>
            <sp:graphNameNode rdf:resource="http://pervasive.semanticweb.org/ont/2013/09/adhocmeeting/models/deviceRoomSectionStore"/>
          </sp:NamedGraph>
          <sp:SubQuery>
            <sp:query>
              <sp:Select>
                <sp:resultVariables rdf:parseType="Collection">
                  <rdf:Description>
                    <sp:expression>
                      <sp:Count>
                        <sp:expression rdf:parseType="Resource">
                          <sp:varName rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                          >S</sp:varName>
                        </sp:expression>
                        <sp:distinct rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean"
                        >true</sp:distinct>
                      </sp:Count>
                    </sp:expression>
                    <sp:varName rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                    >Ct</sp:varName>
                  </rdf:Description>
                  <rdf:Description>
                    <sp:expression>
                      <sp:Avg>
                        <sp:expression rdf:parseType="Resource">
                          <sp:varName rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                          >accS</sp:varName>
                        </sp:expression>
                      </sp:Avg>
                    </sp:expression>
                    <sp:varName rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                    >AvgAccS</sp:varName>
                  </rdf:Description>
                </sp:resultVariables>
                <sp:where rdf:parseType="Collection">
                  <sp:NamedGraph>
                    <sp:elements rdf:parseType="Collection">
                      <rdf:Description>
                        <sp:object rdf:resource="#SensesSkelInPosition"/>
                        <sp:predicate rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#type"/>
                        <sp:subject rdf:parseType="Resource">
                          <sp:varName rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                          >?0</sp:varName>
                        </sp:subject>
                      </rdf:Description>
                      <rdf:Description>
                        <sp:object rdf:parseType="Resource">
                          <sp:varName rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                          >K</sp:varName>
                        </sp:object>
                        <sp:predicate rdf:resource="#kinectCameraRole"/>
                        <sp:subject rdf:parseType="Resource">
                          <sp:varName rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                          >?0</sp:varName>
                        </sp:subject>
                      </rdf:Description>
                      <rdf:Description>
                        <sp:object rdf:parseType="Resource">
                          <sp:varName rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                          >S</sp:varName>
                        </sp:object>
                        <sp:predicate rdf:resource="#skeletonRole"/>
                        <sp:subject rdf:parseType="Resource">
                          <sp:varName rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                          >?0</sp:varName>
                        </sp:subject>
                      </rdf:Description>
                      <rdf:Description>
                        <sp:object rdf:resource="#sitting"/>
                        <sp:predicate rdf:resource="#skelPositionRole"/>
                        <sp:subject rdf:parseType="Resource">
                          <sp:varName rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                          >?0</sp:varName>
                        </sp:subject>
                      </rdf:Description>
                    </sp:elements>
                    <sp:graphNameNode rdf:parseType="Resource">
                      <sp:varName rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                      >gCamera</sp:varName>
                    </sp:graphNameNode>
                  </sp:NamedGraph>
                  <sp:NamedGraph>
                    <sp:elements rdf:parseType="Collection">
                      <rdf:Description>
                        <sp:predicate rdf:resource="http://pervasive.semanticweb.org/ont/2014/05/consert/core#validDuring"/>
                        <sp:object rdf:parseType="Resource">
                          <sp:varName rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                          >validS</sp:varName>
                        </sp:object>
                        <sp:subject rdf:parseType="Resource">
                          <sp:varName rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                          >gCamera</sp:varName>
                        </sp:subject>
                      </rdf:Description>
                      <rdf:Description>
                        <sp:predicate rdf:resource="http://pervasive.semanticweb.org/ont/2014/05/consert/core#hasAccuracy"/>
                        <sp:object rdf:parseType="Resource">
                          <sp:varName rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                          >accS</sp:varName>
                        </sp:object>
                        <sp:subject rdf:parseType="Resource">
                          <sp:varName rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                          >gCamera</sp:varName>
                        </sp:subject>
                      </rdf:Description>
                    </sp:elements>
                    <sp:graphNameNode rdf:resource="http://pervasive.semanticweb.org/ont/2013/09/adhocmeeting/models/SensesSkelInPositionStore"/>
                  </sp:NamedGraph>
                  <sp:Bind>
                    <sp:expression>
                      <cfn:datetimeDelay>
                        <sp:arg1>
                          <afn:now/>
                        </sp:arg1>
                        <sp:arg2 rdf:datatype="http://www.w3.org/2001/XMLSchema#integer"
                        >-300</sp:arg2>
