    <spin:labelTemplate rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >UniquenessConstraintTemplates</spin:labelTemplate>
    <rdfs:subClassOf rdf:resource="#ContextConstraintTemplates"/>
  </ContextConstraintTemplate>
</rdf:RDF>

