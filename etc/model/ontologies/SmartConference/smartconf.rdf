                </sp:object>
              </rdf:Description>
            </sp:elements>
          </sp:NamedGraph>
          <sp:NamedGraph>
            <sp:graphNameNode rdf:resource="http://pervasive.semanticweb.org/ont/2014/01/smartconf/hasNoiseLevelStore"/>
            <sp:elements rdf:parseType="Collection">
              <rdf:Description>
                <sp:subject rdf:parseType="Resource">
                  <sp:varName rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                  >gNoise</sp:varName>
                </sp:subject>
                <sp:object rdf:parseType="Resource">
                  <sp:varName rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                  >time</sp:varName>
                </sp:object>
                <sp:predicate rdf:resource="http://pervasive.semanticweb.org/ont/2014/05/consert/core#hasTimestamp"/>
              </rdf:Description>
              <rdf:Description>
                <sp:subject rdf:parseType="Resource">
                  <sp:varName rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                  >gNoise</sp:varName>
                </sp:subject>
                <sp:object rdf:parseType="Resource">
                  <sp:varName rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                  >accN</sp:varName>
                </sp:object>
                <sp:predicate rdf:resource="http://pervasive.semanticweb.org/ont/2014/05/consert/core#hasAccuracy"/>
