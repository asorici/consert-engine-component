                </sp:subject>
                <sp:predicate rdf:resource="http://pervasive.semanticweb.org/ont/2014/05/consert/core#hasAccuracy"/>
              </rdf:Description>
            </sp:elements>
            <sp:graphNameNode rdf:resource="http://pervasive.semanticweb.org/ont/2014/01/smartconf/hostsAdHocDiscussionStore"/>
