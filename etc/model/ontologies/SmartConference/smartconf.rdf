<?xml version="1.0"?>
<rdf:RDF
    xmlns:digital-doc="http://pervasive.semanticweb.org/ont/2004/06/digital-doc#"
    xmlns:action="http://pervasive.semanticweb.org/ont/2004/06/action#"
    xmlns:event="http://pervasive.semanticweb.org/ont/2004/06/event#"
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:contextassertion="http://pervasive.semanticweb.org/ont/2014/05/consert/core#"
    xmlns:spin="http://spinrdf.org/spin#"
    xmlns:afn="http://jena.hpl.hp.com/ARQ/function#"
    xmlns:cfn="http://pervasive.semanticweb.org/ont/2014/05/consert/functions#"
    xmlns:sp="http://spinrdf.org/sp#"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:person="http://pervasive.semanticweb.org/ont/2004/06/person#"
    xmlns="http://pervasive.semanticweb.org/ont/2014/01/smartconf#"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
    xmlns:schedule="http://pervasive.semanticweb.org/ont/2004/06/schedule#"
    xmlns:device="http://pervasive.semanticweb.org/ont/2004/06/device#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
    xmlns:location="http://pervasive.semanticweb.org/ont/2004/06/location#"
  xml:base="http://pervasive.semanticweb.org/ont/2014/01/smartconf">
  <owl:Ontology rdf:about="">
    <owl:imports rdf:resource="http://pervasive.semanticweb.org/ont/2004/06/digital-doc"/>
    <owl:imports rdf:resource="http://pervasive.semanticweb.org/ont/2004/06/device"/>
    <owl:imports rdf:resource="http://pervasive.semanticweb.org/ont/2004/06/space"/>
    <owl:imports rdf:resource="http://pervasive.semanticweb.org/ont/2004/06/action"/>
    <owl:imports rdf:resource="http://pervasive.semanticweb.org/ont/2004/06/person"/>
    <owl:versionInfo rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Created with TopBraid Composer</owl:versionInfo>
    <owl:imports rdf:resource="http://pervasive.semanticweb.org/ont/2014/05/consert/functions"/>
    <owl:imports rdf:resource="http://pervasive.semanticweb.org/ont/2014/05/consert/core"/>
  </owl:Ontology>
  <owl:Class rdf:ID="ConferencePresentation">
    <rdfs:subClassOf rdf:resource="http://pervasive.semanticweb.org/ont/2004/06/time#IntervalEvent"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >A presentation held during a conference. Presentations are part of sessions.</rdfs:comment>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >ConferencePresentation</rdfs:label>
    <spin:constraint>
      <sp:Construct>
        <sp:where rdf:parseType="Collection">
          <sp:NamedGraph>
            <sp:graphNameNode rdf:parseType="Resource">
              <sp:varName rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
              >g1</sp:varName>
            </sp:graphNameNode>
            <sp:elements rdf:parseType="Collection">
              <rdf:Description>
                <sp:subject rdf:resource="http://spinrdf.org/spin#_this"/>
                <sp:predicate>
                  <owl:ObjectProperty rdf:ID="currentPresentationForSession"/>
                </sp:predicate>
                <sp:object rdf:parseType="Resource">
                  <sp:varName rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                  >Session</sp:varName>
                </sp:object>
              </rdf:Description>
            </sp:elements>
          </sp:NamedGraph>
          <sp:NamedGraph>
            <sp:graphNameNode rdf:resource="http://pervasive.semanticweb.org/ont/2014/01/smartconf/currentPresentationForSessionStore"/>
            <sp:elements rdf:parseType="Collection">
              <rdf:Description>
                <sp:subject rdf:parseType="Resource">
                  <sp:varName rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                  >g1</sp:varName>
                </sp:subject>
                <sp:object rdf:parseType="Resource">
                  <sp:varName rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                  >validity1</sp:varName>
                </sp:object>
                <sp:predicate rdf:resource="http://pervasive.semanticweb.org/ont/2014/05/consert/core#validDuring"/>
              </rdf:Description>
              <rdf:Description>
                <sp:subject rdf:parseType="Resource">
                  <sp:varName rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                  >g1</sp:varName>
                </sp:subject>
                <sp:object rdf:parseType="Resource">
                  <sp:varName rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                  >ts1</sp:varName>
                </sp:object>
                <sp:predicate rdf:resource="http://pervasive.semanticweb.org/ont/2014/05/consert/core#hasTimestamp"/>
              </rdf:Description>
            </sp:elements>
          </sp:NamedGraph>
          <sp:Filter>
            <sp:expression>
              <cfn:validityIntervalExceeds>
                <sp:arg2 rdf:datatype="http://www.w3.org/2001/XMLSchema#integer"
                >2400</sp:arg2>
                <sp:arg1 rdf:parseType="Resource">
                  <sp:varName rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                  >validity1</sp:varName>
                </sp:arg1>
              </cfn:validityIntervalExceeds>
            </sp:expression>
          </sp:Filter>
        </sp:where>
        <sp:templates rdf:parseType="Collection">
          <rdf:Description>
            <sp:subject rdf:nodeID="A0"/>
            <sp:predicate rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#type"/>
            <sp:object rdf:resource="http://spinrdf.org/spin#ConstraintViolation"/>
          </rdf:Description>
          <rdf:Description>
            <sp:subject rdf:nodeID="A0"/>
            <sp:predicate rdf:resource="http://spinrdf.org/spin#violationRoot"/>
            <sp:object rdf:parseType="Resource">
              <sp:varName rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
              >g1</sp:varName>
            </sp:object>
          </rdf:Description>
        </sp:templates>
      </sp:Construct>
    </spin:constraint>
  </owl:Class>
  <owl:Class rdf:ID="Microphone">
    <rdfs:subClassOf rdf:resource="http://pervasive.semanticweb.org/ont/2004/06/device#Device"/>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Microphone</rdfs:label>
  </owl:Class>
  <owl:Class rdf:ID="PresentationSupport">
    <rdfs:subClassOf rdf:resource="http://pervasive.semanticweb.org/ont/2004/06/digital-doc#TextContentFile"/>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >PresentationSupport</rdfs:label>
  </owl:Class>
  <owl:Class rdf:ID="Participant">
    <rdfs:subClassOf>
      <owl:Class rdf:ID="ConferenceRole"/>
    </rdfs:subClassOf>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >A participant to the conference.</rdfs:comment>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Participant</rdfs:label>
    <owl:disjointWith>
      <owl:Class rdf:ID="SessionChair"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:ID="Presenter"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:ID="Staff"/>
    </owl:disjointWith>
  </owl:Class>
  <owl:Class rdf:ID="SessionRoom">
    <rdfs:subClassOf>
      <owl:Class rdf:ID="ConferenceSpace"/>
    </rdfs:subClassOf>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >A conference session room.</rdfs:comment>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >SessionRoom</rdfs:label>
    <owl:disjointWith>
      <owl:Class rdf:ID="CoffeeBreakArea"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:ID="DiscussionArea"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:ID="ReceptionArea"/>
    </owl:disjointWith>
  </owl:Class>
  <owl:Class rdf:about="#ConferenceSpace">
    <rdfs:subClassOf rdf:resource="http://pervasive.semanticweb.org/ont/2004/06/space#SpaceInAFixedStructure"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >A physical space within the conference venue which serves a conference related purpose.</rdfs:comment>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >ConferenceSpace</rdfs:label>
    <owl:unionOf rdf:parseType="Collection">
      <owl:Class rdf:about="#CoffeeBreakArea"/>
      <owl:Class rdf:about="#DiscussionArea"/>
      <owl:Class rdf:about="#ReceptionArea"/>
      <owl:Class rdf:about="#SessionRoom"/>
    </owl:unionOf>
  </owl:Class>
  <owl:Class rdf:about="#DiscussionArea">
    <rdfs:subClassOf rdf:resource="#ConferenceSpace"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >A discussion area reserved for informal discussions between conference participants during the conference breaks.</rdfs:comment>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >DiscussionArea</rdfs:label>
    <owl:disjointWith>
      <owl:Class rdf:about="#CoffeeBreakArea"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:about="#ReceptionArea"/>
    </owl:disjointWith>
    <owl:disjointWith rdf:resource="#SessionRoom"/>
  </owl:Class>
  <owl:Class rdf:about="#CoffeeBreakArea">
    <rdfs:subClassOf rdf:resource="#ConferenceSpace"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >A coffee break area of a conference venue.</rdfs:comment>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >CoffeeBreakArea</rdfs:label>
    <owl:disjointWith rdf:resource="#DiscussionArea"/>
    <owl:disjointWith>
      <owl:Class rdf:about="#ReceptionArea"/>
    </owl:disjointWith>
    <owl:disjointWith rdf:resource="#SessionRoom"/>
  </owl:Class>
  <owl:Class rdf:ID="ConferenceProfile">
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >ConferenceProfile</rdfs:label>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >The profile of a ConferenceUser.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://pervasive.semanticweb.org/ont/2014/05/consert/core#ContextEntity"/>
  </owl:Class>
  <owl:Class rdf:ID="SessionSpeakerArea">
    <owl:disjointWith>
      <owl:Class rdf:ID="SessionAudienceArea"/>
    </owl:disjointWith>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >SessionSpeakerArea</rdfs:label>
    <rdfs:subClassOf>
      <owl:Class>
        <owl:intersectionOf rdf:parseType="Collection">
          <owl:Class rdf:about="#ConferenceSpace"/>
          <owl:Restriction>
            <owl:onProperty rdf:resource="http://pervasive.semanticweb.org/ont/2004/06/space#spatiallySubsumedBy"/>
            <owl:hasValue rdf:resource="#SessionRoom"/>
          </owl:Restriction>
        </owl:intersectionOf>
      </owl:Class>
    </rdfs:subClassOf>
    <rdfs:subClassOf rdf:resource="http://pervasive.semanticweb.org/ont/2014/05/consert/core#ContextEntity"/>
  </owl:Class>
  <owl:Class rdf:about="#SessionChair">
    <rdfs:subClassOf>
      <owl:Class rdf:about="#ConferenceRole"/>
    </rdfs:subClassOf>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >The role of chairing a conference session.</rdfs:comment>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >SessionChair</rdfs:label>
    <owl:disjointWith>
      <owl:Class rdf:about="#Presenter"/>
    </owl:disjointWith>
    <owl:disjointWith rdf:resource="#Participant"/>
    <owl:disjointWith>
      <owl:Class rdf:about="#Staff"/>
    </owl:disjointWith>
  </owl:Class>
  <owl:Class rdf:ID="sensesBadgeWithIntensity">
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:ObjectProperty rdf:ID="sensorRole"/>
        </owl:onProperty>
        <owl:allValuesFrom>
          <owl:Class rdf:ID="ProximitySensor"/>
        </owl:allValuesFrom>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >sensesBadgeWithIntensity</rdfs:label>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="http://pervasive.semanticweb.org/ont/2014/05/consert/core#assertionType"/>
        <owl:hasValue rdf:resource="http://pervasive.semanticweb.org/ont/2014/05/consert/core#Sensed"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:ObjectProperty rdf:ID="badgeRole"/>
        </owl:onProperty>
        <owl:allValuesFrom>
          <owl:Class rdf:ID="ConferenceBadge"/>
        </owl:allValuesFrom>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:ObjectProperty rdf:ID="intensityRole"/>
        </owl:onProperty>
        <owl:allValuesFrom>
          <owl:Class rdf:ID="ProximitySignalIntesity"/>
        </owl:allValuesFrom>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf rdf:resource="http://pervasive.semanticweb.org/ont/2014/05/consert/core#NaryContextAssertion"/>
  </owl:Class>
  <owl:Class rdf:about="#SessionAudienceArea">
    <owl:disjointWith rdf:resource="#SessionSpeakerArea"/>
    <rdfs:subClassOf>
      <owl:Class>
        <owl:intersectionOf rdf:parseType="Collection">
          <owl:Class rdf:about="#ConferenceSpace"/>
          <owl:Restriction>
            <owl:hasValue rdf:resource="#SessionRoom"/>
            <owl:onProperty rdf:resource="http://pervasive.semanticweb.org/ont/2004/06/space#spatiallySubsumedBy"/>
          </owl:Restriction>
        </owl:intersectionOf>
      </owl:Class>
    </rdfs:subClassOf>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >SessionAudienceArea</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://pervasive.semanticweb.org/ont/2014/05/consert/core#ContextEntity"/>
  </owl:Class>
  <owl:Class rdf:about="#ProximitySignalIntesity">
    <owl:oneOf rdf:parseType="Collection">
      <ProximitySignalIntesity rdf:ID="Weak">
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
        >Weak</rdfs:label>
      </ProximitySignalIntesity>
      <ProximitySignalIntesity rdf:ID="Medium">
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
        >Medium</rdfs:label>
      </ProximitySignalIntesity>
      <ProximitySignalIntesity rdf:ID="Strong">
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
        >Strong</rdfs:label>
      </ProximitySignalIntesity>
    </owl:oneOf>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >ProximitySignalIntesity</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://pervasive.semanticweb.org/ont/2014/05/consert/core#ContextEntity"/>
  </owl:Class>
  <owl:Class rdf:ID="hostsAdHocDiscussion">
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="http://pervasive.semanticweb.org/ont/2014/05/consert/core#assertionType"/>
        <owl:hasValue rdf:resource="http://pervasive.semanticweb.org/ont/2014/05/consert/core#Derived"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >hostsAdHocDiscussion</rdfs:label>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:allValuesFrom rdf:resource="#DiscussionArea"/>
        <owl:onProperty rdf:resource="http://pervasive.semanticweb.org/ont/2014/05/consert/core#assertionRole"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf rdf:resource="http://pervasive.semanticweb.org/ont/2014/05/consert/core#UnaryContextAssertion"/>
  </owl:Class>
  <owl:Class rdf:about="#ReceptionArea">
    <rdfs:subClassOf rdf:resource="#ConferenceSpace"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >The conference reception area.</rdfs:comment>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >ReceptionArea</rdfs:label>
    <owl:disjointWith rdf:resource="#CoffeeBreakArea"/>
    <owl:disjointWith rdf:resource="#DiscussionArea"/>
    <owl:disjointWith rdf:resource="#SessionRoom"/>
  </owl:Class>
  <owl:Class rdf:about="#ConferenceBadge">
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >ConferenceBadge</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://pervasive.semanticweb.org/ont/2004/06/device#Device"/>
  </owl:Class>
  <owl:Class rdf:about="#Presenter">
    <rdfs:subClassOf>
      <owl:Class rdf:about="#ConferenceRole"/>
    </rdfs:subClassOf>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >The role of presenting a paper at a conference.</rdfs:comment>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Presenter</rdfs:label>
    <owl:disjointWith rdf:resource="#SessionChair"/>
    <owl:disjointWith rdf:resource="#Participant"/>
    <owl:disjointWith>
      <owl:Class rdf:about="#Staff"/>
    </owl:disjointWith>
  </owl:Class>
  <owl:Class rdf:ID="inAdHocDiscussion">
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:allValuesFrom>
          <rdf:Description rdf:about="http://pervasive.semanticweb.org/ont/2004/06/person#Person">
            <spin:constraint>
              <sp:Construct>
                <sp:templates rdf:parseType="Collection">
                  <rdf:Description>
                    <sp:subject rdf:nodeID="A1"/>
                    <sp:predicate rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#type"/>
                    <sp:object rdf:resource="http://pervasive.semanticweb.org/ont/2014/05/consert/core#UniquenessConstraintViolation"/>
                  </rdf:Description>
                  <rdf:Description>
                    <sp:object>
                      <owl:ObjectProperty rdf:about="http://pervasive.semanticweb.org/ont/2004/06/person#locatedAt"/>
                    </sp:object>
                    <sp:subject rdf:nodeID="A1"/>
                    <sp:predicate rdf:resource="http://pervasive.semanticweb.org/ont/2014/05/consert/core#onContextAssertion"/>
                  </rdf:Description>
                  <rdf:Description>
                    <sp:object rdf:parseType="Resource">
                      <sp:varName rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                      >g1</sp:varName>
                    </sp:object>
                    <sp:subject rdf:nodeID="A1"/>
                    <sp:predicate rdf:resource="http://pervasive.semanticweb.org/ont/2014/05/consert/core#hasConflictingAssertion"/>
                  </rdf:Description>
                  <rdf:Description>
                    <sp:object rdf:parseType="Resource">
                      <sp:varName rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                      >g2</sp:varName>
                    </sp:object>
                    <sp:subject rdf:nodeID="A1"/>
                    <sp:predicate rdf:resource="http://pervasive.semanticweb.org/ont/2014/05/consert/core#hasConflictingAssertion"/>
                  </rdf:Description>
                </sp:templates>
                <sp:where rdf:parseType="Collection">
                  <sp:NamedGraph>
                    <sp:elements rdf:parseType="Collection">
                      <rdf:Description>
                        <sp:object rdf:parseType="Resource">
                          <sp:varName rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                          >Loc1</sp:varName>
                        </sp:object>
                        <sp:predicate>
                          <owl:ObjectProperty rdf:about="http://pervasive.semanticweb.org/ont/2004/06/person#locatedAt"/>
                        </sp:predicate>
                        <sp:subject rdf:resource="http://spinrdf.org/spin#_this"/>
                      </rdf:Description>
                    </sp:elements>
                    <sp:graphNameNode rdf:parseType="Resource">
                      <sp:varName rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                      >g1</sp:varName>
                    </sp:graphNameNode>
                  </sp:NamedGraph>
                  <sp:NamedGraph>
                    <sp:elements rdf:parseType="Collection">
                      <rdf:Description>
                        <sp:object rdf:parseType="Resource">
                          <sp:varName rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                          >Loc2</sp:varName>
                        </sp:object>
                        <sp:predicate>
                          <owl:ObjectProperty rdf:about="http://pervasive.semanticweb.org/ont/2004/06/person#locatedAt"/>
                        </sp:predicate>
                        <sp:subject rdf:resource="http://spinrdf.org/spin#_this"/>
                      </rdf:Description>
                    </sp:elements>
                    <sp:graphNameNode rdf:parseType="Resource">
                      <sp:varName rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                      >g2</sp:varName>
                    </sp:graphNameNode>
                  </sp:NamedGraph>
                  <sp:NamedGraph>
                    <sp:elements rdf:parseType="Collection">
                      <rdf:Description>
                        <sp:object rdf:parseType="Resource">
                          <sp:varName rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                          >validity1</sp:varName>
                        </sp:object>
                        <sp:subject rdf:parseType="Resource">
                          <sp:varName rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                          >g1</sp:varName>
                        </sp:subject>
                        <sp:predicate rdf:resource="http://pervasive.semanticweb.org/ont/2014/05/consert/core#validDuring"/>
                      </rdf:Description>
                      <rdf:Description>
                        <sp:object rdf:parseType="Resource">
                          <sp:varName rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                          >acc1</sp:varName>
                        </sp:object>
                        <sp:subject rdf:parseType="Resource">
                          <sp:varName rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                          >g1</sp:varName>
                        </sp:subject>
                        <sp:predicate rdf:resource="http://pervasive.semanticweb.org/ont/2014/05/consert/core#hasAccuracy"/>
                      </rdf:Description>
                      <rdf:Description>
                        <sp:object rdf:parseType="Resource">
                          <sp:varName rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                          >ts1</sp:varName>
                        </sp:object>
                        <sp:subject rdf:parseType="Resource">
                          <sp:varName rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                          >g1</sp:varName>
                        </sp:subject>
                        <sp:predicate rdf:resource="http://pervasive.semanticweb.org/ont/2014/05/consert/core#hasTimestamp"/>
                      </rdf:Description>
                      <rdf:Description>
                        <sp:object rdf:parseType="Resource">
                          <sp:varName rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                          >validity2</sp:varName>
