            </sp:subject>
          </rdf:Description>
          <rdf:Description>
            <sp:predicate rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#type"/>
            <sp:subject rdf:nodeID="A3"/>
            <sp:object rdf:resource="http://pervasive.semanticweb.org/ont/2014/05/consert/core#ContextAnnotation"/>
