  <spin:ConstructTemplate rdf:ID="hostsAdHocDiscussionRule">
    <rdfs:subClassOf rdf:resource="http://spinrdf.org/spin#ConstructTemplates"/>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >hostsAdHocDiscussionRule</rdfs:label>
    <spin:body>
      <sp:Construct>
        <sp:where rdf:parseType="Collection">
          <sp:NamedGraph>
            <sp:graphNameNode rdf:resource="http://pervasive.semanticweb.org/ont/2014/01/smartconf/entityStore"/>
            <sp:elements rdf:parseType="Collection">
              <rdf:Description>
                <sp:subject rdf:parseType="Resource">
                  <sp:varName rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                  >DA</sp:varName>
                </sp:subject>
                <sp:predicate rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#type"/>
                <sp:object rdf:resource="#DiscussionArea"/>
              </rdf:Description>
              <rdf:Description>
                <sp:subject rdf:parseType="Resource">
                  <sp:varName rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                  >Mic</sp:varName>
                </sp:subject>
                <sp:predicate rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#type"/>
                <sp:object rdf:resource="#Microphone"/>
              </rdf:Description>
              <rdf:Description>
                <sp:subject rdf:parseType="Resource">
                  <sp:varName rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                  >P</sp:varName>
                </sp:subject>
                <sp:predicate rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#type"/>
                <sp:object rdf:resource="http://pervasive.semanticweb.org/ont/2004/06/person#Person"/>
              </rdf:Description>
            </sp:elements>
          </sp:NamedGraph>
          <sp:NamedGraph>
            <sp:graphNameNode rdf:parseType="Resource">
              <sp:varName rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
              >gMicLocation</sp:varName>
            </sp:graphNameNode>
            <sp:elements rdf:parseType="Collection">
              <rdf:Description>
                <sp:subject rdf:parseType="Resource">
                  <sp:varName rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                  >Mic</sp:varName>
                </sp:subject>
                <sp:predicate rdf:resource="http://pervasive.semanticweb.org/ont/2004/06/device#locatedAt"/>
                <sp:object rdf:parseType="Resource">
                  <sp:varName rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                  >DA</sp:varName>
                </sp:object>
              </rdf:Description>
            </sp:elements>
          </sp:NamedGraph>
          <sp:NamedGraph>
            <sp:graphNameNode rdf:resource="http://pervasive.semanticweb.org/ont/2004/06/device/locatedAtStore"/>
            <sp:elements rdf:parseType="Collection">
              <rdf:Description>
                <sp:subject rdf:parseType="Resource">
                  <sp:varName rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                  >gMicLocation</sp:varName>
                </sp:subject>
                <sp:predicate rdf:resource="http://pervasive.semanticweb.org/ont/2014/05/consert/core#assertionType"/>
                <sp:object rdf:resource="http://pervasive.semanticweb.org/ont/2014/05/consert/core#Profiled"/>
              </rdf:Description>
            </sp:elements>
          </sp:NamedGraph>
          <sp:SubQuery>
            <sp:query>
              <sp:Select>
                <sp:where rdf:parseType="Collection">
                  <sp:NamedGraph>
                    <sp:graphNameNode rdf:parseType="Resource">
                      <sp:varName rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                      >gPersonLocation</sp:varName>
                    </sp:graphNameNode>
                    <sp:elements rdf:parseType="Collection">
                      <rdf:Description>
                        <sp:subject rdf:parseType="Resource">
                          <sp:varName rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                          >P</sp:varName>
                        </sp:subject>
                        <sp:predicate rdf:resource="http://pervasive.semanticweb.org/ont/2004/06/person#locatedAt"/>
                        <sp:object rdf:parseType="Resource">
                          <sp:varName rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                          >DA</sp:varName>
                        </sp:object>
                      </rdf:Description>
                    </sp:elements>
                  </sp:NamedGraph>
                  <sp:NamedGraph>
                    <sp:graphNameNode rdf:resource="http://pervasive.semanticweb.org/ont/2004/06/person/locatedAtStore"/>
                    <sp:elements rdf:parseType="Collection">
                      <rdf:Description>
                        <sp:subject rdf:parseType="Resource">
                          <sp:varName rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                          >gPersonLocation</sp:varName>
                        </sp:subject>
                        <sp:object rdf:parseType="Resource">
                          <sp:varName rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                          >validS</sp:varName>
                        </sp:object>
                        <sp:predicate rdf:resource="http://pervasive.semanticweb.org/ont/2014/05/consert/core#validDuring"/>
                      </rdf:Description>
                      <rdf:Description>
                        <sp:subject rdf:parseType="Resource">
                          <sp:varName rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                          >gPersonLocation</sp:varName>
                        </sp:subject>
                        <sp:object rdf:parseType="Resource">
                          <sp:varName rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                          >accS</sp:varName>
                        </sp:object>
                        <sp:predicate rdf:resource="http://pervasive.semanticweb.org/ont/2014/05/consert/core#hasAccuracy"/>
                      </rdf:Description>
                    </sp:elements>
                  </sp:NamedGraph>
                  <sp:Bind>
                    <sp:variable rdf:parseType="Resource">
                      <sp:varName rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                      >earlier</sp:varName>
                    </sp:variable>
                    <sp:expression>
                      <cfn:datetimeDelay>
                        <sp:arg2 rdf:datatype="http://www.w3.org/2001/XMLSchema#integer"
                        >-300</sp:arg2>
                        <sp:arg1>
                          <afn:now/>
                        </sp:arg1>
                      </cfn:datetimeDelay>
                    </sp:expression>
                  </sp:Bind>
                  <sp:Bind>
                    <sp:variable rdf:parseType="Resource">
                      <sp:varName rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                      >interv</sp:varName>
                    </sp:variable>
                    <sp:expression>
                      <cfn:makeValidityInterval>
                        <sp:arg2>
                          <afn:now/>
                        </sp:arg2>
                        <sp:arg1 rdf:parseType="Resource">
                          <sp:varName rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                          >earlier</sp:varName>
                        </sp:arg1>
                      </cfn:makeValidityInterval>
                    </sp:expression>
                  </sp:Bind>
                  <sp:Filter>
                    <sp:expression>
                      <cfn:validityIntervalsInclude>
                        <sp:arg2 rdf:parseType="Resource">
                          <sp:varName rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                          >interv</sp:varName>
                        </sp:arg2>
                        <sp:arg1 rdf:parseType="Resource">
                          <sp:varName rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                          >validS</sp:varName>
                        </sp:arg1>
                      </cfn:validityIntervalsInclude>
                    </sp:expression>
                  </sp:Filter>
                </sp:where>
                <sp:resultVariables rdf:parseType="Collection">
                  <rdf:Description>
                    <sp:varName rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                    >Ct</sp:varName>
                    <sp:expression>
                      <sp:Count>
                        <sp:distinct rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean"
                        >true</sp:distinct>
                        <sp:expression rdf:parseType="Resource">
                          <sp:varName rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                          >P</sp:varName>
                        </sp:expression>
                      </sp:Count>
                    </sp:expression>
                  </rdf:Description>
                  <rdf:Description>
                    <sp:varName rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                    >AvgAccS</sp:varName>
                    <sp:expression>
                      <sp:Avg>
                        <sp:expression rdf:parseType="Resource">
                          <sp:varName rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                          >accS</sp:varName>
                        </sp:expression>
                      </sp:Avg>
                    </sp:expression>
                  </rdf:Description>
                </sp:resultVariables>
              </sp:Select>
            </sp:query>
          </sp:SubQuery>
          <sp:Filter>
            <sp:expression>
              <sp:and>
                <sp:arg1>
                  <sp:ge>
                    <sp:arg1 rdf:parseType="Resource">
                      <sp:varName rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                      >Ct</sp:varName>
                    </sp:arg1>
                    <sp:arg2 rdf:datatype="http://www.w3.org/2001/XMLSchema#integer"
                    >2</sp:arg2>
                  </sp:ge>
                </sp:arg1>
                <sp:arg2>
                  <sp:ge>
                    <sp:arg1 rdf:parseType="Resource">
                      <sp:varName rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                      >AvgAccS</sp:varName>
                    </sp:arg1>
                    <sp:arg2 rdf:datatype="http://www.w3.org/2001/XMLSchema#decimal"
                    >0.75</sp:arg2>
                  </sp:ge>
                </sp:arg2>
              </sp:and>
            </sp:expression>
          </sp:Filter>
          <sp:NamedGraph>
            <sp:graphNameNode rdf:parseType="Resource">
              <sp:varName rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
              >gNoise</sp:varName>
            </sp:graphNameNode>
            <sp:elements rdf:parseType="Collection">
              <rdf:Description>
                <sp:subject rdf:parseType="Resource">
                  <sp:varName rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                  >Mic</sp:varName>
                </sp:subject>
                <sp:predicate rdf:resource="#hasNoiseLevel"/>
